{
  "name": "Picture-in-Picture",
  "version": "1.0.0",
  "description": "onlkwanvnivnnasf",
  "permissions": ["storage", "tabs", "<all_urls>", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
